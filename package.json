{
  "name": "pronnect",
  "version": "1.0.0",
  "description": "social network for students",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "pronect-install": "npm install --prefix pronect",
    "pronect": "npm start --prefix pronect",
    "dev": "concurrently \"npm run server\" \"npm run pronect\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix pronect && npm run build --prefix pronect"
  },
  "author": "ayan sengupta",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "express": "^4.16.4",
    "express-fileupload": "^1.0.0",
    "fs": "0.0.1-security",
    "gravatar": "^1.6.0",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.3.12",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "validator": "^10.9.0"
  },
  "devDependencies": {
    "nodemon": "^1.18.6"
  }
}
